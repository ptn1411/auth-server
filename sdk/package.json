{
  "name": "auth-server-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for Auth Server API",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "test": "jest --runInBand --forceExit",
    "test:coverage": "jest --runInBand --forceExit --coverage",
    "test:watch": "jest --watch",
    "prepublishOnly": "npm run build",
    "clean": "rm -rf dist coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/user/auth-server.git",
    "directory": "sdk"
  },
  "keywords": [
    "auth",
    "authentication",
    "authorization",
    "api",
    "sdk",
    "typescript",
    "jwt",
    "oauth2",
    "mfa",
    "webauthn",
    "passkey"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/user/auth-server/issues"
  },
  "homepage": "https://github.com/user/auth-server/tree/main/sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.3.0"
  }
}
